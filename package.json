{
  "name": "my-steedos-project",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "steedos-packages/*"
  ],
  "scripts": {
    "docker": "docker-compose up",
    "start": "moleculer-runner steedos-packages/*/package.service.js --hot --repl",
    "repl": "moleculer-runner --repl",
    "nodered": "node-red --settings nodered.config.js"
  },
  "dependencies": {
    "@steedos/node-red-contrib-steedos": "^2.4.0",
    "@steedos/service-object-mixin": "^2.5.0-beta.6",
    "@steedos/service-package-loader": "^2.5.0-beta.6",
    "dotenv-flow": "^3.2.0",
    "moleculer-repl": "^0.7.3",
    "node-red": "^3.0.2"
  }
}
